{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/app.js",
  "scripts": {
    "start": "node --unhandled-rejections=strict ./dist/app.js",
    "dev": "nodemon ./src/app.ts",
    "build": "tsc && uglifyjs-folder ./dist -e -x .js -o dist",
    "watch": "tsc --watch",
    "debug": "export DEBUG=* && npm run start",
    "web": "node --optimize_for_size --max_old_space_size=448 --gc_interval=100 --unhandled-rejections=strict ./dist/app.js",
    "lint": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.29.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "shortid": "^2.2.16",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.19",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "uglifyjs-folder": "^3.1.3"
  }
}
