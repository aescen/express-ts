{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/app.js",
  "scripts": {
    "dist": "cross-env NODE_ENV=production NODE_PATH=./ npm run start:dist",
    "dev:dist": "cross-env NODE_ENV=development NODE_PATH=./ DEBUG=* npm run start:dist",
    "start:dist": "node --unhandled-rejections=strict ./dist/app.js",
    "start": "cross-env NODE_ENV=production NODE_PATH=./ npm run start:build",
    "start:build": "npm run build && node --unhandled-rejections=strict ./dist/app.js",
    "web": "cross-env NODE_ENV=production NODE_PATH=./ npm run start:web",
    "start:web": "node --optimize_for_size --max_old_space_size=448 --gc_interval=100 --unhandled-rejections=strict ./dist/app.js",
    "dev": "cross-env NODE_ENV=development NODE_PATH=./ DEBUG=* npm run start:dev",
    "start:dev": "npm run start:build",
    "dev:watch": "cross-env NODE_ENV=development NODE_PATH=./ DEBUG=* npm run start:dev:watch",
    "start:dev:watch": "nodemon ./src/app.ts",
    "test": "cross-env NODE_ENV=test npm run start:test",
    "start:test": "mocha -r ts-node/register 'test/**/*.test.ts' --unhandled-rejections=strict",
    "test:debug": "cross-env DEBUG=* npm test",
    "build": "tsc && uglifyjs-folder ./dist -e -x .js -o dist",
    "ts:watch": "tsc --watch",
    "lint": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.29.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.0",
    "shortid": "^2.2.16",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "chai": "^4.3.6",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "uglifyjs-folder": "^3.1.3"
  }
}
